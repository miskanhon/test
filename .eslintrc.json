{
    "extends":"standard",
    "ignoreComments":true,
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "env": {
        "node": false,
        "browser": true,
        "es6": true
    },
    "rules":{
        "no-unused-vars":["error", { "vars": "local" }],
        "semi":["error", "always"],
        "eol-last":["error", "never"]
    }
}